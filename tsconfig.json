{
	"extends": "@react-native/typescript-config/tsconfig.json",
	"compilerOptions": {
		"types": ["jest", "node"],
		"paths": {
			"@/*": ["./src/*", "./src/*/index", "./src/*/android", "./src/*/ios"]
		},
		"moduleSuffixes": [".ios", ".android", ""],
		"allowImportingTsExtensions": false
	},
	"include": [
		"./src/**/*",
		"./src/@types/**/*",
		"index.d.ts",
		".eslintrc.js",
		"jest.setup.js",
		"jest.config.js",
		"babel.config.js",
		"react-native.config.js",
		"index.js"
	]
}
